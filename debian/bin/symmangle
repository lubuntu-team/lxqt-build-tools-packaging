if [ -d ./debian ]; then
    for i in `find . -name symbols`; do
        k=`echo $i | sed "s#/DEBIAN/symbols##" | sed "s#./debian/##"`
        cat "$i" | sed 's/ \(_.*\) \(.*\)/ (c++)"\1" \2/' | c++filt | sort | uniq | tee "debian/$k.mangled";
    done
fi
